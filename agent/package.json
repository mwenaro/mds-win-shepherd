{
  "name": "win-management-agent",
  "version": "1.0.0",
  "description": "Windows management agent service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc && pkg dist/index.js --target win --output agent.exe",
    "start": "node dist/index.js",
    "compile": "tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/ws": "^8.5.5",
    "pkg": "^5.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
